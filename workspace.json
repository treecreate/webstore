{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "strict": true
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "projects": {
    "api": {
      "projectType": "application",
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "targets": {
        "run": {
          "executor": "@nxrocks/nx-spring-boot:run",
          "options": {
            "root": "apps/api"
          }
        },
        "serve": {
          "executor": "@nxrocks/nx-spring-boot:serve",
          "options": {
            "root": "apps/api"
          }
        },
        "test": {
          "executor": "@nxrocks/nx-spring-boot:test",
          "options": {
            "root": "apps/api"
          }
        },
        "clean": {
          "executor": "@nxrocks/nx-spring-boot:clean",
          "options": {
            "root": "apps/api"
          }
        },
        "buildJar": {
          "executor": "@nxrocks/nx-spring-boot:buildJar",
          "options": {
            "root": "apps/api"
          }
        },
        "buildWar": {
          "executor": "@nxrocks/nx-spring-boot:buildWar",
          "options": {
            "root": "apps/api"
          }
        },
        "buildImage": {
          "executor": "@nxrocks/nx-spring-boot:buildImage",
          "options": {
            "root": "apps/api"
          }
        },
        "buildInfo": {
          "executor": "@nxrocks/nx-spring-boot:buildInfo",
          "options": {
            "root": "apps/api"
          }
        }
      }
    },
    "webstore": {
      "projectType": "application",
      "root": "apps/webstore",
      "sourceRoot": "apps/webstore/src",
      "prefix": "webstore",
      "i18n": {
        "sourceLocale": "en-US",
        "locales": {
          "dk": "apps/webstore/src/i18n/messages.dk.xlf"
        }
      },
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/webstore",
            "index": "apps/webstore/src/index.html",
            "main": "apps/webstore/src/main.ts",
            "polyfills": "apps/webstore/src/polyfills.ts",
            "tsConfig": "apps/webstore/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/webstore/src/favicon.ico",
              "apps/webstore/src/assets",
              {
                "input": "libs/assets/src/lib",
                "glob": "**/*",
                "output": "assets"
              }
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "apps/webstore/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/webstore/src/environments/environment.ts",
                  "with": "apps/webstore/src/environments/environment.prod.ts"
                }
              ],
              "index": {
                "input": "apps/webstore/src/index.prod.html",
                "output": "index.html"
              },
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "dk": {
              "fileReplacements": [
                {
                  "replace": "apps/webstore/src/environments/environment.ts",
                  "with": "apps/webstore/src/environments/environment.dk.ts"
                }
              ],
              "localize": [
                "dk"
              ]
            }
          }
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "webstore:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "webstore:build:production"
            },
            "dk": {
              "browserTarget": "webstore:build:dk"
            }
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "webstore:build",
            "outputPath": "apps/webstore/src/i18n/"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/webstore/src/**/*.ts",
              "apps/webstore/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/webstore"
          ],
          "options": {
            "jestConfig": "apps/webstore/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "webstore-e2e": {
      "root": "apps/webstore-e2e",
      "sourceRoot": "apps/webstore-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/webstore-e2e/cypress.json",
            "tsConfig": "apps/webstore-e2e/tsconfig.e2e.json",
            "devServerTarget": "webstore:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "webstore:serve:production"
            },
            "dk": {
              "devServerTarget": "webstore:serve:dk"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/webstore-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "models": {
      "root": "libs/models",
      "sourceRoot": "libs/models/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/models/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/models"
          ],
          "options": {
            "jestConfig": "libs/models/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "interfaces": {
      "root": "libs/interfaces",
      "sourceRoot": "libs/interfaces/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/interfaces/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/interfaces"
          ],
          "options": {
            "jestConfig": "libs/interfaces/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "mocks": {
      "projectType": "library",
      "root": "libs/mocks",
      "sourceRoot": "libs/mocks/src",
      "prefix": "webstore",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/mocks/src/**/*.ts",
              "libs/mocks/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/mocks"
          ],
          "options": {
            "jestConfig": "libs/mocks/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "assets": {
      "root": "libs/assets",
      "sourceRoot": "libs/assets/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/assets/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/assets"
          ],
          "options": {
            "jestConfig": "libs/assets/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "admin-page": {
      "projectType": "application",
      "root": "apps/admin-page",
      "sourceRoot": "apps/admin-page/src",
      "prefix": "webstore",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/admin-page",
            "index": "apps/admin-page/src/index.html",
            "main": "apps/admin-page/src/main.ts",
            "polyfills": "apps/admin-page/src/polyfills.ts",
            "tsConfig": "apps/admin-page/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/admin-page/src/favicon.ico",
              "apps/admin-page/src/assets"
            ],
            "styles": [
              "apps/admin-page/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/admin-page/src/environments/environment.ts",
                  "with": "apps/admin-page/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "admin-page:build",
            "proxyConfig": "apps/admin-page/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "admin-page:build:production"
            }
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "admin-page:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/admin-page/src/**/*.ts",
              "apps/admin-page/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/admin-page"
          ],
          "options": {
            "jestConfig": "apps/admin-page/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "admin-page-e2e": {
      "root": "apps/admin-page-e2e",
      "sourceRoot": "apps/admin-page-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/admin-page-e2e/cypress.json",
            "tsConfig": "apps/admin-page-e2e/tsconfig.e2e.json",
            "devServerTarget": "admin-page:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "admin-page:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/admin-page-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  }
}